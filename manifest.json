{
  "name": "Delayed Search",
  "description": "Extension to delay searches in google, youtube, etc.",
  "version": "1.0.0",
  "manifest_version": 3,
  "options_page": "options/options.html",
  "icons": {
    "16": "favicon.png",
    "32": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "worker.js",
    "type": "module"
  },
  "action": {
    "default_title": "Delay Search",
    "default_icon": "favicon.png"
  }
}
